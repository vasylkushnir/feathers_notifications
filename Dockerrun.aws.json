{
  "AWSEBDockerrunVersion": 2,
    "volumes": [
       {
         "name": "postgres",
         "host": {
           "sourcePath": "/data/db"
         }
       } 
     ],
     "containerDefinitions": [
       {
         "name": "notifications",
         "image": "231089140187/notifications_notifications-api",
         "essential": true,
         "memory": 128,
         "portMappings": [
           {
             "hostPort": 80,
             "containerPort": 3030
           }
         ],
         "links": ["postgres"]
       },
       {
         "name": "postgres",
         "image": "postgres:10.4",
         "memory": 128,
         "portMappings": [
           {
             "hostPort": 5432,
             "containerPort": 5432
           }
         ]
       }
     ]
   } 